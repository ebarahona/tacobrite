<% if logged_in? %>
  <script type="text/javascript">
    window.currentUser = <%= render(
      "api/users/user.json.jbuilder",
      user: current_user
    ).html_safe %>
  </script>
<% end %>

<script type="text/javascript">
  window.images = {
    logo: "<%= asset_path('EventliterLogo.png') %>",
    logo_footer: "<%= asset_path('logo_footer.png') %>",
    concert: "<%= asset_path('concert.jpg') %>",
    sf: "<%= asset_path('sf.jpg') %>",
    user: "<%= asset_path('user.png') %>",
    fav: "<%= asset_path('favicon.png') %>",
    linkedin: "<%= asset_path('linkedin.png') %>",
    github: "<%= asset_path('github.png') %>",
  }
</script>

<main id='root'></main>
